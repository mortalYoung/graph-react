{
  "private": false,
  "name": "graph-react",
  "homepage": "http://mortalYoung.github.io/react-graph",
  "version": "0.0.1",
  "author": "mortalYoung",
  "license": "ISC",
  "description": "a graph generator based on mxGraph",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "father-build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "commit": "git-cz",
    "predeploy": "npm run docs:build",
    "deploy": "gh-pages -d build"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mortalYoung/react-graph"
  },
  "bugs": {
    "url": "https://github.com/mortalYoung/react-graph/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "mxgraph": "^4.1.1",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/node": "^14.0.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "dumi": "^1.0.6",
    "father-build": "^1.17.2",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  },
  "keywords": [
    "react",
    "mxgraph",
    "react-graph",
    "graph",
    "react-component"
  ]
}
